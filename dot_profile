# Set PATH so it includes user's private bin
export PATH="$HOME/bin:$HOME/.local/bin:$PATH:$HOME/go/bin"

# Add Brew to PATH
eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"

# Set micro as default editor
export EDITOR="$(which micro)"

# Added by Toolbox App
export PATH="$PATH:/home/jp/.local/share/JetBrains/Toolbox/scripts"

# Add asdf to PATH
source /home/linuxbrew/.linuxbrew/opt/asdf/libexec/asdf.sh

# Configure overmind
export OVERMIND_NO_PORT=1

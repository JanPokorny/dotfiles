# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.local/bin" ] ; then
    PATH="$HOME/.local/bin:$PATH"
fi

# Set-up Brew
eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv)"

# Disable tpm2-software warning https://github.com/tpm2-software/tpm2-pkcs11/issues/792
export TPM2_PKCS11_LOG_LEVEL=0

# Added by Toolbox App
export PATH="$PATH:/home/jp/.local/share/JetBrains/Toolbox/scripts"

# Add docker plugins to PATH
export PATH="$PATH:/usr/libexec/docker/cli-plugins"
